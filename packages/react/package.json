  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "pnpm typecheck && unbuild",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@elegant-router/core": "workspace:*",
    "consola": "3.2.3",
    "gradient-string": "^2.0.2",
    "magicast": "0.3.4",
    "unplugin": "1.10.1"
  },
  "devDependencies": {
    "@types/gradient-string": "^1.1.6",
    "@types/node": "20.12.12",
    "react-router-dom": "^6.25.1",
    "typescript": "5.4.5",
    "unbuild": "2.0.0",
    "vite": "5.2.11"
  }
